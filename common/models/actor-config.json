{
  "name": "ActorConfig",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "preferredUsername": {
      "type": "string",
      "required": true
    },
    "displayName": {
      "type": "string",
      "required": true
    },
    "summary": {
      "type": "string",
      "required": true
    },
    "location": {
      "type": "geopoint"
    },
    "privacy": {
      "type": "string",
      "required": true,
      "default": "public"
    },
    "otpKey": {
      "type": "string"
    },
    "require2fa": {
      "type": "boolean"
    },
    "defaultLang": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "iconMedia": {
      "type": "hasOne",
      "model": "Media",
      "foreignKey": "iconHash"
    },
    "headerMedia": {
      "type": "hasOne",
      "model": "Media",
      "foreignKey": "headerHash"
    },
    "actor": {
      "type": "belongsTo",
      "model": "Actor",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
