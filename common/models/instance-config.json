{
  "name": "InstanceConfig",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "summary": {
      "type": "string"
    },
    "terms": {
      "type": "string"
    },
    "contentWarning": {
      "type": "string"
    },
    "federationType": {
      "type": "string",
      "required": true,
      "default": "https://www.w3.org/ns/activitystreams"
    },
    "scope": {
      "type": "string",
      "required": true,
      "default": "https://www.w3.org/ns/activitystreams#Public"
    },
    "disableMedia": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "defaultLang": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "logoMedia": {
      "type": "hasOne",
      "model": "Media",
      "foreignKey": ""
    },
    "instance": {
      "type": "belongsTo",
      "model": "Instance",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
