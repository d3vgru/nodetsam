{
  "name": "Note",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "forceId": false,
  "properties": {
    "hash": {
      "type": "string",
      "id": true
    },
    "hashType": {
      "type": "string"
    },
    "canonicalAddress": {
      "type": "string"
    },
    "warning": {
      "type": "string"
    },
    "content": {
      "type": "string",
      "required": true
    },
    "timestamp": {
      "type": "date",
      "required": true
    },
    "location": {
      "type": "geopoint"
    },
    "likeCount": {
      "type": "number",
      "default": 0
    },
    "repostCount": {
      "type": "number",
      "default": 0
    },
    "nativeFormat": {
      "type": "string",
      "required": true,
      "default": "native"
    },
    "scope": {
      "type": "string",
      "required": true,
      "default": "https://www.w3.org/ns/activitystreams#Public"
    },
    "lang": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "actor": {
      "type": "belongsTo",
      "model": "Actor",
      "foreignKey": ""
    },
    "media": {
      "type": "hasMany",
      "model": "Media",
      "foreignKey": "",
      "through": "NoteMedia"
    },
    "parent": {
      "type": "hasOne",
      "model": "Note",
      "foreignKey": "parentNoteId"
    },
    "root": {
      "type": "hasOne",
      "model": "Note",
      "foreignKey": "rootNoteId"
    }
  },
  "acls": [],
  "methods": {}
}
