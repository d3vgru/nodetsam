{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "AccessToken": {
    "dataSource": "pgsql",
    "public": false,
    "relations": {
      "user": {
        "type": "belongsTo",
        "model": "Actor",
        "foreignKey": "actorId"
      }
    }
  },
  "ACL": {
    "dataSource": "pgsql",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "pgsql",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "pgsql",
    "public": false
  },
  "Actor": {
    "dataSource": "pgsql",
    "public": true,
    "options": {
      "emailVerificationRequired": true
    }
  },
  "Instance": {
    "dataSource": "pgsql",
    "public": true
  },
  "Note": {
    "dataSource": "pgsql",
    "public": true
  },
  "Media": {
    "dataSource": "pgsql",
    "public": true
  },
  "InstanceConfig": {
    "dataSource": "pgsql",
    "public": true
  },
  "ActorConfig": {
    "dataSource": "pgsql",
    "public": true
  },
  "Attachment": {
    "dataSource": "storage",
    "public": true
  },
  "Block": {
    "dataSource": "pgsql",
    "public": true
  },
  "Mute": {
    "dataSource": "pgsql",
    "public": true
  },
  "Subscriber": {
    "dataSource": "pgsql",
    "public": true
  },
  "Subscription": {
    "dataSource": "pgsql",
    "public": true
  },
  "NoteMedia": {
    "dataSource": "pgsql",
    "public": true
  },
  "Like": {
    "dataSource": "pgsql",
    "public": true
  }
}
