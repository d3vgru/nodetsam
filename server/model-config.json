{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mysql",
    "options": {
      "emailVerificationRequired": true
    }
  },
  "AccessToken": {
    "dataSource": "mysql",
    "public": false
  },
  "ACL": {
    "dataSource": "mysql",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mysql",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mysql",
    "public": false
  },
  "Actor": {
    "dataSource": "mysql",
    "public": true
  },
  "Instance": {
    "dataSource": "mysql",
    "public": true
  },
  "Post": {
    "dataSource": "mysql",
    "public": true
  },
  "Media": {
    "dataSource": "mysql",
    "public": true
  },
  "InstanceConfig": {
    "dataSource": "mysql",
    "public": true
  },
  "ActorConfig": {
    "dataSource": "mysql",
    "public": true
  },
  "Attachment": {
    "dataSource": "storage",
    "public": true
  }
}
